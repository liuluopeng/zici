{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist",
                "buildCommand": "npm install && npm run cnchar-prod && npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "$1"
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    }
}