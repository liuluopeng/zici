{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "$1"
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        },
        "commands": [
            "npm install",
            "npm run cnchar-prod",
            "npm run build"
        ],
        "outputDirectory": "dist"
    }
}